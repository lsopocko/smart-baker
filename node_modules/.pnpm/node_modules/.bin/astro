#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mal/repositories/smart-baker/node_modules/.pnpm/astro@5.9.2_@netlify+blobs@8.2.0_@types+node@20.19.0_jiti@2.4.2_lightningcss@1.30.1_rol_af7a1985662dae2648839b29fa015803/node_modules/astro/node_modules:/Users/mal/repositories/smart-baker/node_modules/.pnpm/astro@5.9.2_@netlify+blobs@8.2.0_@types+node@20.19.0_jiti@2.4.2_lightningcss@1.30.1_rol_af7a1985662dae2648839b29fa015803/node_modules:/Users/mal/repositories/smart-baker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mal/repositories/smart-baker/node_modules/.pnpm/astro@5.9.2_@netlify+blobs@8.2.0_@types+node@20.19.0_jiti@2.4.2_lightningcss@1.30.1_rol_af7a1985662dae2648839b29fa015803/node_modules/astro/node_modules:/Users/mal/repositories/smart-baker/node_modules/.pnpm/astro@5.9.2_@netlify+blobs@8.2.0_@types+node@20.19.0_jiti@2.4.2_lightningcss@1.30.1_rol_af7a1985662dae2648839b29fa015803/node_modules:/Users/mal/repositories/smart-baker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
